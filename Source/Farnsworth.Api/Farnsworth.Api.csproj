  <PropertyGroup Label="Package">
    <Product>Farnsworth</Product>
    <Description>Quick way to build  MQ based distributed .NET applications</Description>
    <PackageTags>Tag1;Tag2;Tag3</PackageTags>
  </PropertyGroup>

</Project>
